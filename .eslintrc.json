{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "error",
        "no-duplicate-imports": "error",
        "no-plusplus": "off",
        "no-underscore-dangle": ["error", {"allow": "_id"}],
        "no-console": "error"
    }
}
